    <% layout('layouts/boilerplate') %> <%# Only need to include this partial in once place. %> 
        <div id="map" style="width: 100%; height: 500px"></div>
    <h1>These are the gym locations!</h1> 
    <div>
        <a href="/gymlocations/new">Add Gym Location</a>
    </div>
    
        <%# Want to provide linked to each gym locations, the logic is as follows  %> 
            <% for (let gymlocation of gymlocations) { %> 
        <div class="card" mb-3>
            <div class="row">
                <div class="col-md-4">
                        <% if(gymlocation.images.length) { %>
                    <img class="img-fluid" alt="" src="<%= gymlocation.images[0].url %>" crossorigin>
                        <% } else { %> 
                        <img class="img-fluid" alt="" src="https://res.cloudinary.com/doxd8y17p/image/upload/v1666827247/SacaGym/tprycqw1bqgozhyqggfz.jpg" crossorigin>
                        <% } %> 
                </div>
                    <div class="col-md-8">
                        <div class="card-body">
                            <h5 class="card-title"><%= gymlocation.title %></h5>
                            <p class="card-text"><%= gymlocation.description %></p>
                            <p class="card-text">
                                <small class="text-muted"><%= gymlocation.location %></small>
                            </p>
                            <a href="/gymlocations/<%= gymlocation._id%>"class="btn btn-primary">View <%= gymlocation.title %> </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
                <% } %> 
